using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Metric_Converter
{
    class Program
    {
        static void Main(string[] args)
        {
            double numToConvert = double.Parse(Console.ReadLine());
            String fromMetric = Console.ReadLine();
            String toMetric = Console.ReadLine();
            double result = 0;

            if (fromMetric == "m")
            {
                if (toMetric == "m")
                {
                    result = numToConvert;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert * 1.0936133;
                }
            }
            else if (fromMetric == "mm")
            {
                if (toMetric == "m")
                {
                    result = numToConvert / 1000;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 10;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert / 1000 * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 1000 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert / 1000 * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert / 1000 * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert / 1000 * 1.0936133;
                }
            }
            else if (fromMetric == "cm")
            {
                if (toMetric == "m")
                {
                    result = numToConvert / 100;
