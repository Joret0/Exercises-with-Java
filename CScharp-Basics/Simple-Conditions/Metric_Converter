            {
                if (toMetric == "m")
                {
                    result = numToConvert / 0.000621371192;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert / 0.000621371192 * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 0.000621371192 * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 0.000621371192 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert / 0.000621371192 * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert / 0.000621371192 * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert / 0.000621371192 * 1.0936133;
                }
            }
            else if (fromMetric == "in")
            {
                if (toMetric == "m")
                {
                    result = numToConvert / 39.3700787;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert / 39.3700787 * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 39.3700787 * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert / 39.3700787 * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 39.3700787 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert / 39.3700787 * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert / 39.3700787 * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert / 39.3700787 * 1.0936133;
                }
            }
            else if (fromMetric == "km")
            {
                if (toMetric == "m")
                {
                    result = numToConvert / 0.001;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert / 0.001 * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 0.001 * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert / 0.001 * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 0.001 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert / 0.001 * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert / 0.001 * 1.0936133;
                }
            }
            else if (fromMetric == "ft")
            {
                if (toMetric == "m")
                {
                    result = numToConvert / 3.2808399;
                }
                else if (toMetric == "mm")
                {
                    result = numToConvert / 3.2808399 * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 3.2808399 * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert / 3.2808399 * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 3.2808399 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert / 3.2808399 * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert / 3.2808399 * 1.0936133;
                }
            }
