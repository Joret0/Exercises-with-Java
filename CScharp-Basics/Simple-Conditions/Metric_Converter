                else if (toMetric == "mm")
                {
                    result = numToConvert / 1.0936133 * 1000;
                }
                else if (toMetric == "cm")
                {
                    result = numToConvert / 1.0936133 * 100;
                }
                else if (toMetric == "mi")
                {
                    result = numToConvert / 1.0936133 * 0.000621371192;
                }
                else if (toMetric == "in")
                {
                    result = numToConvert / 1.0936133 * 39.3700787;
                }
                else if (toMetric == "km")
                {
                    result = numToConvert / 1.0936133 * 0.001;
                }
                else if (toMetric == "ft")
                {
                    result = numToConvert / 1.0936133 * 3.2808399;
                }
                else if (toMetric == "yd")
                {
                    result = numToConvert;
                }
            }
            Console.WriteLine("{0} {1}", result, toMetric);
