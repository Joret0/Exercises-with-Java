using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace pr04Elevator
{
    class Elevator
    {
        static void Main(string[] args)
        {
            double numberOfPeople = double.Parse(Console.ReadLine());
            double capacityOfPeople = double.Parse(Console.ReadLine());
            double coursesOfElevator = numberOfPeople / capacityOfPeople;
            Console.WriteLine((int)Math.Ceiling(coursesOfElevator));

        }
    }
}
